{
  "npm": false,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "sources": [
    "data/**/*",
    "src/**/*"
  ],
  "entrypoint": "index.html",
  "shell": "src/cards/card-view.js",
  "fragments": [
    "src/components/card-input.js",
    "src/components/card-modal.js"
  ],
  "builds": [{
    "bundle": true,
    "js": {
      "minify": true
    },
    "css": {
      "minify": true
    },
    "html": {
      "minify": true
    },
    "addServiceWorker": true
  }]
}